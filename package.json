{
	"name": "@caiquearaujo/cause-effect-js",
	"version": "1.0.0",
	"description": "A pure JS application to cause-effect through the interface.",
	"scripts": {
		"ts:check": "tsc --noEmit",
		"ts:check-watch": "npm run ts:check -- --watch",
		"ts:build": "tsc --emitDeclarationOnly",
		"js:watch": "rimraf ./public/dist && rollup -c rollup.config.dev.js -w",
		"js:serve": "rimraf ./public/dist && rollup -c rollup.config.dev.js && nodemon ./public/serve.js",
		"js:umd": "rimraf ./dist && rollup -c",
		"js:cjs": "rimraf ./lib && tsc --module commonjs --outDir lib",
		"js:esm": "rimraf ./esm && tsc --module ESNext --outDir esm",
		"lint": "eslint --ext .ts,.js --ignore-path .gitignore --fix src",
		"format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
		"build": "npm run css:deploy && npm run build:cjs && npm run build:esm && npm run build:umd",
		"prepare": "npm run build",
		"version": "npm run format && git add -A src",
		"postversion": "git push && git push --tags",
		"prepublishOnly": "npm test && npm run lint",
		"preversion": "npm run lint",
		"test": "jest",
		"test:debug": "node --inspect node_modules/.bin/jest --runInBand"
	},
	"author": "caiquearaujo",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/caiquearaujo/challenge-cause-effect-js.git"
	},
	"bugs": {
		"url": "https://github.com/caiquearaujo/challenge-cause-effect-js/issues"
	},
	"homepage": "https://github.com/caiquearaujo/challenge-cause-effect-js#readme",
	"devDependencies": {
		"@lopatnov/rollup-plugin-uglify": "^2.1.2",
		"@types/jest": "^27.5.1",
		"@types/node": "^17.0.33",
		"@typescript-eslint/eslint-plugin": "^5.23.0",
		"@typescript-eslint/parser": "^5.23.0",
		"autoprefixer": "^10.4.7",
		"clean-css-cli": "^5.6.0",
		"eslint": "^8.15.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"express": "^4.18.1",
		"jest": "^28.1.0",
		"jest-environment-jsdom": "^28.1.0",
		"nodemon": "^2.0.16",
		"postcss-cli": "^9.1.0",
		"postcss-preset-env": "^7.5.0",
		"prettier": "^2.6.2",
		"rimraf": "^3.0.2",
		"sass": "^1.51.0",
		"ts-jest": "^28.0.2",
		"ts-loader": "^9.3.0",
		"typescript": "^4.6.4"
	},
	"dependencies": {
		"@rollup/plugin-node-resolve": "^13.3.0",
		"@rollup/plugin-typescript": "^8.3.2",
		"rollup": "^2.73.0",
		"rollup-plugin-scss": "^3.0.0"
	}
}
